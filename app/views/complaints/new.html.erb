<%=form_for :complaint, url: complaints_path , remote: true do |f|%>
    <p>
      <%= f.label :title %><br>
      <%= f.text_field :title %>
    </p>

    <p>
      <%= f.label :text %><br>
      <%= f.text_area :text %>
    </p>

    <p>
      <%= f.label :report_to_condo_management %><br>
      <%= f.check_box :active%>
    </p>


    <p>
      <%= f.submit %>
    </p>
  <% end %>
